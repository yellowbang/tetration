  "scripts": {
    "start": "npm-run-all -p watch-css start-dev-server",
    "build-dev": "npm-run-all prebuild build-css webpack-development",
    "build-prod": "npm-run-all prebuild build-css webpack-production",
    "preview": "npm run prebuild && NODE_ENV=\"production\" webpack-dev-server",
    "start-dev-server": "webpack-dev-server",
    "webpack-production": "NODE_ENV=\"production\" webpack",
    "webpack-development": "NODE_ENV=\"development\" webpack",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "prebuild": "babel-node ./scripts/prebuild.js",
    "test": "karma start ./test/karma.conf.js"
  },
